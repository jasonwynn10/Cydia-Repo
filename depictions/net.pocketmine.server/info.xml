<package>
	<id>net.pocketmine.server</id>
	<name>PocketMine-MP for iOS</name>
	<version>1.3.12</version>
	<compatibility>
		<firmware>
			<miniOS>7.0</miniOS>
		</firmware>
	</compatibility>
	<dependencies>
		<package>curl</package>
		<package>grep</package>
		<package>coreutils</package>
		<package>findutils</package>
		<package>gzip</package>
		<package>sed</package>
		<package>shell-cmds</package>
		<package>tar</package>
		<package>net.pocketmine.php-5.5</package>
		<package>screen</package>
	</dependencies>
	<descriptionlist>
		<description>PocketMine-MP is the open-source server software for Minecraft: Pocket Edition.
			This package installs the latest version into /var/mobile/Media/PocketMine-MP/
			You will need access to the Terminal to run this server, you can get it on Cydia.</description>
	</descriptionlist>
	<screenshots>
	</screenshots>
	<changelog>
		<change>Initial Release</change>
		<change> </change>
	</changelog>
	<links>
		<link>
			<name>/r/pocketmine</name>
			<url>https://www.reddit.com/r/pocketmine</url>
			<iconclass>fa fa-reddit</iconclass>
		</link>
	</links>
</package>